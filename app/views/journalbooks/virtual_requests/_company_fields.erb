<h2 class="company-header">Contact Information</h2>
<div class="row pad-top">
  <div class="four-col">  
    <%= f.label :contact_name, "Requested by" %>
    <%= f.text_field :contact_name %>
  </div>
  <div class="four-col">  
    <%= f.label :company, "Distributor" %>
    <%= f.text_field :company %>
  </div>
  <div class="four-col">
    <%= f.label :contact_email, "Contact Email" %>
    <%= f.text_field :contact_email %>
  </div>
  <div class="four-col">
    <%= f.label :contact_phone, "Contact Phone" %>
    <%= f.text_field :contact_phone %>
  </div>
</div>
<div class="row pad-bottom-large">
  <div class="four-col">  
    <%= f.label :asi_number, "Customer Number" %>
    <%= f.text_field :asi_number %>
  </div>
</div>
